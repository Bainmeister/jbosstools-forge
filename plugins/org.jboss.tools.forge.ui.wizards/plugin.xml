<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
    <extension
          point="org.eclipse.ui.newWizards">
       <wizard
             name="New Project"
             category="org.jboss.ide.eclipse.ui.wizards"
             class="org.jboss.tools.forge.ui.wizard.newproject.NewProjectWizard"
             id="org.jboss.tools.forge.ui.wizard.newproject">
          <description>
                Create a new project
          </description>
       </wizard>
       <wizard
             name="Set Up Persistence"
             category="org.jboss.ide.eclipse.ui.wizards"
             class="org.jboss.tools.forge.ui.wizard.persistence.PersistenceSetupWizard"
             id="org.jboss.tools.forge.ui.wizard.persistence">
          <description>
                Set up persistence
          </description>
       </wizard>
       <wizard
             name="Scaffold Setup"
             category="org.jboss.ide.eclipse.ui.wizards"
             class="org.jboss.tools.forge.ui.wizard.scaffold.ScaffoldSetupWizard"
             id="org.jboss.tools.forge.ui.wizard.scaffold.setup">
          <description>
                Set up scaffolding
          </description>
       </wizard>
       <wizard
             name="Scaffold Entities"
             category="org.jboss.ide.eclipse.ui.wizards"
             class="org.jboss.tools.forge.ui.wizard.scaffold.ScaffoldWizard"
             id="org.jboss.tools.forge.ui.wizard.scaffold.jsf">
          <description>
                Scaffold entities
